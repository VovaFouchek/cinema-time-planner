    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.5.1",
    "json-server": "^0.17.4",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "react-select": "^5.7.7"
